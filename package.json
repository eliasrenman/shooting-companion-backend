{
  "name": "shooting-companion",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "private": true,
  "version": "1.0.0",
  "bugs": {
    "url": "https://github.com/eliasrenman/shooting-companion-backend/issues"
  },
  "homepage": "https://github.com/eliasrenman/shooting-companion-backend#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eliasrenman/shooting-companion-backend.git"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:prod": "npm run build",
    "lint": "eslint --ext .js,.ts src/**",
    "lint:fix": "npm run lint -- --fix",
    "pre-push": "npm run lint && npm run test && npm run test:e2e && npm run build:prod && npm run tpl-repo:check",
    "prebuild": "rimraf dist",
    "preinstall": "node ./tools/check-yarn.js",
    "start": "ts-node src/main.ts",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/src/main.js\"",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/src/main.js\"",
    "start:prod": "node dist/src/main.js",
    "test": "jest --config jest.config.js",
    "test:cov": "npm run test -- --coverage",
    "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand --config jest.config.js",
    "test:e2e": "jest --config ./test/jest-e2e.config.js",
    "test:watch": "npm run test -- --watch",
    "tpl-repo": "tools/merge-with-repository-template.sh",
    "tpl-repo:check": "npm run tpl-repo -- check",
    "tpl-repo:merge": "npm run tpl-repo -- merge"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.0.0",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mongoose": "^7.2.4",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^4.8.0",
    "@nozbe/watermelondb": "^0.23.0",
    "@types/express-serve-static-core": "^4.17.1",
    "@types/passport-facebook": "^2.1.10",
    "@types/uuid": "^8.3.0",
    "bcrypt": "^5.0.1",
    "express": "^4.17.1",
    "joi": "^17.4.0",
    "loadsh": "^0.0.4",
    "module-alias": "~2.2.2",
    "mongoose": "^5.12.4",
    "morgan": "^1.10.0",
    "nanoid": "^3.1.25",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "query-string": "^7.0.1",
    "reflect-metadata": "~0.1.13",
    "rimraf": "~3.0.0",
    "rxjs": "~6.5.3",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@nestjs/cli": "~6.10.2",
    "@nestjs/schematics": "~6.7.0",
    "@nestjs/testing": "~6.8.0",
    "@types/bcrypt": "^3.0.1",
    "@types/jest": "~25.1.0",
    "@types/lodash": "^4.14.172",
    "@types/module-alias": "~2.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^14.14.41",
    "@types/passport-google-oauth20": "^2.0.7",
    "@types/passport-jwt": "^3.0.5",
    "@types/supertest": "~2.0.8",
    "@typescript-eslint/eslint-plugin": "~2.18.0",
    "@typescript-eslint/parser": "~2.18.0",
    "eslint": "~6.8.0",
    "eslint-config-airbnb-base": "~14.0.0",
    "eslint-import-resolver-typescript": "~2.0.0",
    "eslint-plugin-filenames": "~1.3.2",
    "eslint-plugin-import": "~2.20.0",
    "eslint-plugin-promise": "~4.2.1",
    "eslint-plugin-unicorn": "~15.0.1",
    "husky": "~3.0.8",
    "jest": "~25.1.0",
    "jest-extended": "~0.11.5",
    "supertest": "~4.0.2",
    "ts-jest": "~25.0.0",
    "ts-loader": "~6.2.0",
    "ts-node": "~8.6.2",
    "tsc-watch": "~4.1.0",
    "typescript": "~3.7.5"
  },
  "husky": {
    "hooks": {}
  }
}
